// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int a,b,x,y,hcf,lcm;
   printf("enter the first number:");
   scanf("%d",&a);
   printf("enter the second number:");
   scanf("%d",&b);
   for(int i=1;i<=a && i<=b; i++){
       if(a%i==0 && b%i==0){
           hcf=i;
            
       }
   }
    printf("hcf of %d and %d= %d\n",a,b,hcf);
    lcm=(a*b)/hcf;
       printf("lcm of %d and %d= %d",a,b,lcm);
       return 0;
}
       
   
  
