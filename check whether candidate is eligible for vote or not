// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int age;
  char name[10];
    printf(" Enter the name of the candidate:\n");
    scanf("%s",&name);
    printf(" Enter the age of the candidate:\n");
    scanf("%d",&age);
    if(age>=18){
         printf(" %s is eligible to vote:", name);
   
    }
    else{
        printf(" %s is not eligible to vote:", name); 
    }
    
   
    return 0;
}
