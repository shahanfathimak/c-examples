// Online C compiler to run C program online
#include <stdio.h>
int main() {
  int n,n1,n2,nextterm;
printf("enter the number of terms:\n");
scanf("%d",&n);
  n1=0;
  n2=1;
  nextterm=n1+n2;
  
  printf("fibonacci series:%d,%d,",n1,n2);       
  for(int i=3;i<=n;i++){
      printf("%d,", nextterm);
      n1=n2;
      n2=nextterm;
      nextterm=n1+n2;
      
      
  }
       return 0;
}
       
   
  
