// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
char a[20]="xray";
char b[20]="zord";
int c;
int i=0;
while(a[i]!='\0'&& b[i]!='\0'){
if(a[i]!=b[i]){
    c=a[i]-b[i];
    break;
}
i++;

}
c=a[i]-b[i];
printf("%d\n",c);
if(c==0){
  printf("string a=string b")  ;
}
else if(c<0){
printf("string a less than string b")  ;
}
else{
    printf("string a greater than string b")  ;
}


return 0;
}
       
   
  
