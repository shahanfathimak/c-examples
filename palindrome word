// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
char a[50];
printf("enter the word\n");
scanf("%s",&a);
int l=strlen(a);
printf("%d\n",l);
char b[l];
int i;
for(i=0;i<l;i++){
    b[i]=a[l-1-i];
}
b[l]='\0';
printf("%s\n",b);
if(strcmp(a,b)==0){
    printf("word is palindrome");
}
else{
    printf("word is not palindrome");
}

return 0;
}
       
   
  
