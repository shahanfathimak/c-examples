// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int x[4];
    int *p=&x[0];
    printf("enter the variables:");
    
    int i;
  for(i=0; i<4;i++){
         scanf("%d",p+i);
        
  }
    for(i=0; i<4;i++){
         printf("%d ",*(p+i));
        
  }
     
     
    
       return 0;
}
       
   
  
