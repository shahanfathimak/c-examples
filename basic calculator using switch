// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n1,n2;
    char op;
    int result;
    printf("enter the operator:\n");
    scanf("%c",&op);
    printf("enter the first number:\n");
    scanf("%d",&n1);
    printf("enter the second number:\n");
    scanf("%d",&n2);
    switch(op){
        case '+':
        printf("%d + %d=%d",n1,n2, n1+n2);
        break;
        case '-':
        printf("%d - %d=%d",n1,n2, n1-n2);
        break;
        case '*':
        printf("%d * %d=%d",n1,n2, n1*n2);
        break;
        case '/':
        printf("%d / %d=%d",n1,n2, n1/n2);
        break;
        default:
         printf(" Invalid operator");
    }

    return 0;
}
