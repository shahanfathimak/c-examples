#include <stdio.h>

int main() {
   int a,b,x,y,hcm,lcm;
   printf("enter the first number:");
   scanf("%d",&a);
   printf("enter the second number:");
   scanf("%d",&b);
   for(int i=1;i<=a && i<=b; i++){
       if(a%i==0 && b%i==0){
           hcm=i;
       }
   }
    printf("hcf of %d and %d= %d",a,b,hcm);
       return 0;
}
